<body>
  <div class="attendio-desktop-faculty-recor" data-animate-on-scroll>
    <div class="rectangles">
      <div class="rectangles-child"></div>
      <div class="rectangles-item"></div>
      <div class="rectangles-inner"></div>
      <div class="rectangles-inner"></div>
    </div>
    <div class="recorded">
      <img class="checked-1-iconc" alt="" src="/public/images/checkmark.png" />

      <b class="attendance-recorded">
        <p class="attendance">ATTENDANCE</p>
        <p class="attendance">RECORDED!</p>
      </b>
    </div>
    <button class="home-button" id="homeButton" onclick="window.location.href='/DS'" >
      <div class="home-button-child"></div>
      <div class="icon-home-parent">
        <img class="icon-home" alt="" src="/images/home.svg" />
        <div class="home">HOME</div>
      </div>
    </button>
    <div class="present-absent">
      <div class="present-absent-child"></div>
      <div class="present-absent-parent">
        <div class="present-absent1">
          <p class="attendance">
            <span>Present:</span>
            <span class="span"> </span>
          </p>
          <p class="attendance">
            <span>
              <span>Absent:</span>
              <span class="span1"> </span>
            </span>
            <span class="span1">
              <span class="span3"> </span>
            </span>
          </p>
        </div>
        <div class="div">
          <p class="attendance">1</p>
          <p class="attendance">0</p>
        </div>
      </div>
    </div>
  </div>

